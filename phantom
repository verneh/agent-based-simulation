; the idea here is to observe a phantom traffic jam.
; we simulate traffic which resembles the one on this video.
; https://www.youtube.com/watch?v=Q78Kb4uLAdA
; or this - https://www.youtube.com/watch?v=ZNLIoolCeKI

globals [
  sample-car
  sample-building
]

breed [buildings building]
breed [spots spot]
breed [tracks track]
breed [automobiles automobile]

automobiles-own [
  speed
  speed-limit
  speed-min
]

to setup

  clear-all
  reset-ticks
  setup-bg
  setup-building
  setup-tracks
  setup-automobiles
  watch sample-car

end

to setup-bg
   ask patches [
   set pcolor 54
  ]
   ask n-of 50 patches [
    set pcolor 55
  ]
  ask n-of 100 patches [
    set pcolor 56
  ]



end

to setup-building

end

to setup-automobiles

  set-default-shape turtles "car top"
  create-automobiles cars [
    set size 4
    forward 20
    set heading heading + 89.5 ; set new heading to be old heading + x degrees.
    set speed 0.1 + random-float 0.9
    set speed-min 0
    set speed-limit 1

  ]
    set sample-car one-of automobiles
    ask sample-car [ set color violet ]


end


to setup-tracks ; create main track
  create-tracks 1 [
    set shape "circle"
    set size 46
    set color 58
    stamp
    set size 36
    ask tracks [
      set color scale-color 57 25 0 50
    ]
    stamp
  ]
end




to go
  ; set the ticks to "slower"
  run-automobiles
end




to run-automobiles ; determines how our cars go around the track.

  ask automobiles [

    let car-ahead one-of other automobiles in-cone 1 60 ; 1 jump and angle at 60 degrees
    ifelse car-ahead != nobody
     [set speed [speed] of car-ahead - deceleration] ; if there is a car ahead, decelerate speed to be the speed of the car ahead.
     [set speed speed + acceleration] ; if not set speed to be a little faster.

    if speed < speed-min [set speed speed-min] ; if speed is less than zero, set it to zero.
    if speed > speed-limit [set speed speed-limit] ; we don't get faster than max speed.

    ; this allows cars to stay on the circular track
    forward ((pi * 40) / 360) * speed
    set heading heading + speed
  ]
  display

end
@#$#@#$#@
